        <Label x:Name="var2" Style="{DynamicResource Labels}" Content="dwewe" Margin="270,196,156,185" Height="Auto" Width="Auto" FontSize="18"/>
        <Label x:Name="var3" Style="{DynamicResource Labels}" Content="dwewe" Margin="270,232,156,149" Height="Auto" Width="Auto" FontSize="18"/>
        <Label x:Name="var4" Style="{DynamicResource Labels}" Content="dwewe" Margin="270,268,156,113" Height="Auto" Width="Auto" FontSize="18"/>
        <Button Content="Далее" Style="{DynamicResource reg}"  Margin="321,342,321,24" FontSize="18" Click="Button_Click" FontFamily="Franklin Gothic Demi"/>

    </Grid>
</Window>
